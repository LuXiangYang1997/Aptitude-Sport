<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data></data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/color_FFFFFF"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_30"
                android:src="@mipmap/icon_chenggong" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/dp_10"
                android:layout_marginBottom="@dimen/dp_30"
                android:gravity="center"
                android:text="恭喜您完成比赛"
                android:textColor="@color/color_FF8F00"
                android:textSize="@dimen/sp_14" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_10"
            android:background="@color/color_FFFFFF"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_30"
                android:layout_marginTop="@dimen/dp_15"
                android:background="@color/color_FFEED9"
                android:gravity="center_vertical|center_horizontal"
                android:text="成绩详情"
                android:textColor="@color/color_FF8F00" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/ll_matchOne"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_35"
                    android:orientation="horizontal">

                    <TextView

                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_horizontal|center_vertical"
                        android:singleLine="true"
                        android:text="比赛项名称"
                        android:textColor="@color/color_666666"
                        android:textSize="@dimen/sp_11" />

                    <View
                        android:layout_width="@dimen/dp_0_5"
                        android:layout_height="match_parent"
                        android:background="@color/color_E2E2E2" />

                    <TextView
                        android:id="@+id/matchprogram_name"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:maxLines="2"
                        android:text=""
                        android:textSize="@dimen/sp_11" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_0_5"
                    android:background="@color/color_E2E2E2" />

                <LinearLayout
                    android:id="@+id/ll_matchtwo"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_35"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_horizontal|center_vertical"
                        android:singleLine="true"
                        android:text="分组名称"
                        android:textColor="@color/color_666666"
                        android:textSize="@dimen/sp_11" />

                    <View
                        android:layout_width="@dimen/dp_0_5"
                        android:layout_height="match_parent"
                        android:background="@color/color_E2E2E2" />

                    <TextView
                        android:id="@+id/match_groupname"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:maxLines="2"
                        android:textSize="@dimen/sp_11" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_0_5"
                    android:background="@color/color_E2E2E2" />

                <LinearLayout
                    android:id="@+id/ll_matchthree"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_35"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_horizontal|center_vertical"
                        android:singleLine="true"
                        android:text="赛场名称"
                        android:textColor="@color/color_666666"
                        android:textSize="@dimen/sp_11" />

                    <View
                        android:layout_width="@dimen/dp_0_5"
                        android:layout_height="match_parent"
                        android:background="@color/color_E2E2E2" />

                    <TextView
                        android:id="@+id/matchlocation_name"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:maxLines="2"
                        android:textSize="@dimen/sp_11" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_0_5"
                    android:background="@color/color_E2E2E2" />

                <LinearLayout
                    android:id="@+id/ll_matchfour"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_35"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_horizontal|center_vertical"
                        android:singleLine="true"
                        android:text="赛事名称"
                        android:textColor="@color/color_666666"
                        android:textSize="@dimen/sp_11" />

                    <View
                        android:layout_width="@dimen/dp_0_5"
                        android:layout_height="match_parent"
                        android:background="@color/color_E2E2E2" />

                    <TextView
                        android:id="@+id/match_name"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:maxLines="2"
                        android:textSize="@dimen/sp_11" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_0_5"
                    android:background="@color/color_E2E2E2" />
            </LinearLayout>

        </LinearLayout>


    </LinearLayout>


</layout>
