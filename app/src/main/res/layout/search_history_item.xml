<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="searchHistoryAdapter"
            type="com.huasport.smartsport.ui.matchapply.adapter.SearchHistoryAdapter" />

        <variable
            name="position"
            type="int" />

        <variable
            name="searchTestBean"
            type="com.huasport.smartsport.ui.matchapply.bean.SearchHistoryTestBean" />

    </data>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >

        <TextView
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/searchhistory_itemstyle"
            android:maxLines="1"
            android:paddingBottom="@dimen/dp_5"
            android:paddingLeft="@dimen/dp_10"
            android:paddingRight="@dimen/dp_10"
            android:paddingTop="@dimen/dp_5"
            android:text="@{searchTestBean.edittext}"
            android:textColor="@color/color_999999"
            android:textSize="@dimen/sp_14" />


    </LinearLayout>
</layout>
